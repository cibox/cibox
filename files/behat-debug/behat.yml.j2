default:
  paths:
    features: 'features'
  extensions:
    Behat\MinkExtension\Extension:
      goutte: ~
      selenium2:
        wd_host: 127.0.0.1:4444/wd/hub
        browser: firefox
      base_url: {{ behat_base_url }}
    Drupal\DrupalExtension\Extension:
      blackbox: ~
      api_driver: 'drupal'
      drush:
        alias: 'local'
      drupal:
        drupal_root: {{ behat_drupal_root }}
